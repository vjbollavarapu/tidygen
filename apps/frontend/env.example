# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1
VITE_APP_NAME=TidyGen
VITE_APP_VERSION=1.0.0

# Authentication
VITE_JWT_SECRET=your-jwt-secret-key
VITE_TOKEN_STORAGE_KEY=tidygen_token

# Payment Integration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id
VITE_PAYPAL_ENVIRONMENT=sandbox

# Web3 Configuration (if needed)
VITE_WEB3_PROVIDER_URL=https://mainnet.infura.io/v3/your-project-id
VITE_WEB3_CHAIN_ID=1

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CRASH_REPORTING=true
VITE_ENABLE_PAYMENTS=true

# Development
VITE_DEBUG=false
VITE_LOG_LEVEL=info

# Production URLs (for Vercel deployment)
# VITE_API_BASE_URL=https://your-backend-api.vercel.app/api/v1
# VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_live_stripe_key
# VITE_PAYPAL_ENVIRONMENT=live
